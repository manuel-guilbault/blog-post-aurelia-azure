{
  "$schema": "http://json.schemastore.org/proxies",
  "proxies": {
      "default": {
          "matchCondition": {
              "route": "/{*path}"
          },
          "backendUri": "https://%webAppContentStorageAndContainer%/{path}"
      },
      "root": {
          "matchCondition": {
              "route": "/"
          },
          "backendUri": "https://%webAppContentStorageAndContainer%/index.html"
      }
  }
}

{
  "$schema": "http://json.schemastore.org/proxies",
  "proxies": {
      "default": {
          "matchCondition": {
              "route": "/{*path}"
          },
          "backendUri": "%StorageContainer.Uri%/{path}%StorageContainer.SasToken%"
      },
      "root": {
          "matchCondition": {
              "route": "/"
          },
          "backendUri": "%StorageContainer.Uri%/index.html%StorageContainer.SasToken%"
      }
  }
}

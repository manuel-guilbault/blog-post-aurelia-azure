{
  "$schema": "http://json.schemastore.org/proxies",
  "proxies": {
      "default": {
          "matchCondition": {
              "route": "/{*path}"
          },
          "backendUri": "https://%StorageContainer.HostAndContainer%/{path}%StorageContainer.SasToken%"
      },
      "root": {
          "matchCondition": {
              "route": "/"
          },
          "backendUri": "https://%StorageContainer.HostAndContainer%/index.html%StorageContainer.SasToken%"
      }
  }
}
